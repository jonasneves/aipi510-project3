[Unit]
Description=MLflow Tracking UI
After=network.target

[Service]
Type=simple
User=ubuntu
WorkingDirectory=/home/ubuntu/aipi510-project3-claude
ExecStart=/home/ubuntu/aipi510-project3-claude/.venv/bin/mlflow ui --host 0.0.0.0 --port 5000
Restart=always
RestartSec=3

[Install]
WantedBy=multi-user.target
